# Database Configuration Examples
# Copy this file to .env and fill in your actual values

# Distribution Database
DB_DISTRIBUTION_HOST=your-server.database.windows.net
DB_DISTRIBUTION_PORT=1433
DB_DISTRIBUTION_DATABASE=your-database-name
DB_DISTRIBUTION_USERNAME=your-username
DB_DISTRIBUTION_PASSWORD=your-password

# Warehouse Database
DB_WAREHOUSE_HOST=your-warehouse-server.database.windows.net
DB_WAREHOUSE_PORT=1433
DB_WAREHOUSE_DATABASE=your-warehouse-database
DB_WAREHOUSE_USERNAME=your-username
DB_WAREHOUSE_PASSWORD=your-password

# Monday.com API (if used)
MONDAY_API_TOKEN=your-monday-api-token

# Other configurations
LOG_LEVEL=INFO
ENVIRONMENT=development
